{
  "name": "eslint-config-ackama",
  "version": "1.1.0",
  "description": "Standard ESLint configurations for Ackama projects.",
  "homepage": "https://github.com/ackama/eslint-config-ackama#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ackama/eslint-config-ackama.git"
  },
  "license": "ISC",
  "author": "Gareth Jones",
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "eslint . --ignore-pattern '!.eslintrc.js' --ext js,ts",
    "release": "semantic-release -r git+ssh://git@github.com/squareweave/eslint-config-ackama.git",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": "prettier-config-ackama",
  "dependencies": {
    "eslint-config-prettier": "^6.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^7.0.18",
    "eslint": "^6.8.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "prettier-config-ackama": "^0.1.2",
    "semantic-release": "^15.14.0"
  },
  "peerDependencies": {
    "eslint": ">= 6",
    "eslint-plugin-eslint-comments": ">= 3",
    "eslint-plugin-prettier": ">= 3.1",
    "prettier": ">= 1.19"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
